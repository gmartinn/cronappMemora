        <class>app.entity.AuditLog</class>
        <class>app.entity.RoleSecurable</class>
        <class>app.entity.UserSecurable</class>
        <class>app.entity.TBL_HUB_IDIOMAS</class>
        <class>app.entity.Application</class>
